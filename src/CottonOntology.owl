        <rdf:type rdf:resource="http://www.semanticweb.org/sainath/ontologies/2016/11/CottonOntology#Fungal_disease"/>
        <affectsPart rdf:resource="http://www.semanticweb.org/sainath/ontologies/2016/11/CottonOntology#Boll"/>
        <affectsPart rdf:resource="http://www.semanticweb.org/sainath/ontologies/2016/11/CottonOntology#Leaves"/>
        <hasSymptom rdf:resource="http://www.semanticweb.org/sainath/ontologies/2016/11/CottonOntology#Brown_circular_lesions_redish_borders"/>
        <hasSymptom rdf:resource="http://www.semanticweb.org/sainath/ontologies/2016/11/CottonOntology#Concentric_rings_on_bolls"/>
        <hasSymptom rdf:resource="http://www.semanticweb.org/sainath/ontologies/2016/11/CottonOntology#Extensive_defoliation"/>
        <isCausedBy rdf:resource="http://www.semanticweb.org/sainath/ontologies/2016/11/CottonOntology#Alternaria_gossypii"/>
